{
  "dashboard": {
    "id": null,
    "title": "Sweet Athena Monitoring Dashboard",
    "tags": ["sweet-athena", "ai-assistant", "monitoring"],
    "style": "dark",
    "timezone": "browser",
    "editable": true,
    "graphTooltip": 1,
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
    },
    "refresh": "30s",
    "version": 1,
    "panels": [
      {
        "id": 1,
        "title": "Sweet Athena Overview",
        "type": "stat",
        "targets": [
          {
            "expr": "increase(athena_interactions_total[5m])",
            "legendFormat": "Interactions (5m)",
            "refId": "A"
          },
          {
            "expr": "rate(athena_interactions_total[5m]) * 60",
            "legendFormat": "Interactions/min",
            "refId": "B"
          },
          {
            "expr": "athena_sweetness_level",
            "legendFormat": "Avg Sweetness",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "min": 0,
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "displayMode": "list",
              "orientation": "horizontal"
            }
          }
        },
        "gridPos": {
          "h": 6,
          "w": 24,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Sweet Athena Interactions by Type",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (interaction_type) (increase(athena_interactions_total[1h]))",
            "legendFormat": "{{interaction_type}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "displayMode": "list"
            }
          }
        },
        "options": {
          "pieType": "pie",
          "tooltip": {
            "mode": "single"
          },
          "legend": {
            "displayMode": "visible",
            "placement": "bottom"
          },
          "displayLabels": ["name", "percent"]
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 6
        }
      },
      {
        "id": 3,
        "title": "Personality Mood Distribution",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (personality_mood) (increase(athena_interactions_total[1h]))",
            "legendFormat": "{{personality_mood}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "options": {
          "pieType": "donut",
          "tooltip": {
            "mode": "single"
          },
          "legend": {
            "displayMode": "visible",
            "placement": "bottom"
          },
          "displayLabels": ["name", "percent"]
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 6
        }
      },
      {
        "id": 4,
        "title": "Sweet Athena Response Time",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(athena_response_time_seconds_bucket[5m]))",
            "legendFormat": "95th percentile",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(athena_response_time_seconds_bucket[5m]))",
            "legendFormat": "50th percentile (median)",
            "refId": "B"
          },
          {
            "expr": "rate(athena_response_time_seconds_sum[5m]) / rate(athena_response_time_seconds_count[5m])",
            "legendFormat": "Average",
            "refId": "C"
          }
        ],
        "yAxes": [
          {
            "unit": "s",
            "min": 0
          }
        ],
        "xAxis": {
          "mode": "time"
        },
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 14
        },
        "fill": 2,
        "lineWidth": 2,
        "pointSize": 5,
        "showGrid": true,
        "tooltip": {
          "shared": true,
          "sort": 0
        }
      },
      {
        "id": 5,
        "title": "Sweetness Level Over Time",
        "type": "graph",
        "targets": [
          {
            "expr": "avg by (personality_mood) (athena_sweetness_level)",
            "legendFormat": "{{personality_mood}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "unit": "short",
            "min": 1,
            "max": 10
          }
        ],
        "xAxis": {
          "mode": "time"
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 22
        },
        "fill": 2,
        "lineWidth": 2,
        "pointSize": 5
      },
      {
        "id": 6,
        "title": "Avatar Animation Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "athena_animation_frame_rate",
            "legendFormat": "FPS - {{animation_type}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(athena_avatar_render_time_ms_bucket[5m]))",
            "legendFormat": "95th percentile render time",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "unit": "short",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 22
        }
      },
      {
        "id": 7,
        "title": "User Satisfaction Scores",
        "type": "heatmap",
        "targets": [
          {
            "expr": "sum by (le) (rate(athena_user_satisfaction_bucket[5m]))",
            "legendFormat": "{{le}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              }
            }
          }
        },
        "options": {
          "calculate": false,
          "cellGap": 2,
          "cellValues": {},
          "color": {
            "exponent": 0.5,
            "fill": "dark-orange",
            "mode": "spectrum",
            "reverse": false,
            "scale": "exponential",
            "scheme": "RdYlGn",
            "steps": 64
          },
          "exemplars": {
            "color": "rgba(255,0,255,0.7)"
          },
          "filterValues": {
            "le": 1e-9
          },
          "legend": {
            "show": true
          },
          "rowsFrame": {
            "layout": "auto"
          },
          "tooltip": {
            "show": true,
            "yHistogram": true
          },
          "yAxis": {
            "axisPlacement": "left",
            "reverse": false,
            "unit": "short"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 30
        }
      },
      {
        "id": 8,
        "title": "Conversation Metrics",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(athena_conversation_length_bucket[5m]))",
            "legendFormat": "95th percentile conversation length",
            "refId": "A"
          },
          {
            "expr": "rate(athena_conversation_length_sum[5m]) / rate(athena_conversation_length_count[5m])",
            "legendFormat": "Average conversation length",
            "refId": "B"
          },
          {
            "expr": "rate(athena_interactions_total[5m]) * 60",
            "legendFormat": "Interactions per minute",
            "refId": "C"
          }
        ],
        "yAxes": [
          {
            "unit": "short",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 38
        }
      },
      {
        "id": 9,
        "title": "System Performance Impact",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(memory_usage_bytes{type=\"heap_used\"}[5m])",
            "legendFormat": "Memory usage rate",
            "refId": "A"
          },
          {
            "expr": "rate(cpu_usage_percent[5m])",
            "legendFormat": "CPU usage rate",
            "refId": "B"
          },
          {
            "expr": "rate(http_request_duration_seconds_sum{route=~\".*athena.*\"}[5m]) / rate(http_request_duration_seconds_count{route=~\".*athena.*\"}[5m])",
            "legendFormat": "Average Athena API response time",
            "refId": "C"
          }
        ],
        "yAxes": [
          {
            "unit": "short",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 46
        }
      },
      {
        "id": 10,
        "title": "Error Rates",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total{route=~\".*athena.*\",status_code=~\"4..\"}[5m])",
            "legendFormat": "4xx errors",
            "refId": "A"
          },
          {
            "expr": "rate(http_requests_total{route=~\".*athena.*\",status_code=~\"5..\"}[5m])",
            "legendFormat": "5xx errors",
            "refId": "B"
          },
          {
            "expr": "rate(http_requests_total{route=~\".*athena.*\",status_code=~\"2..\"}[5m])",
            "legendFormat": "2xx success",
            "refId": "C"
          }
        ],
        "yAxes": [
          {
            "unit": "reqps",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 54
        }
      }
    ],
    "templating": {
      "list": [
        {
          "allValue": null,
          "current": {
            "text": "All",
            "value": "$__all"
          },
          "datasource": "Prometheus",
          "definition": "label_values(athena_interactions_total, personality_mood)",
          "hide": 0,
          "includeAll": true,
          "label": "Personality Mood",
          "multi": true,
          "name": "mood",
          "options": [],
          "query": "label_values(athena_interactions_total, personality_mood)",
          "refresh": 1,
          "regex": "",
          "skipUrlSync": false,
          "sort": 1,
          "tagValuesQuery": "",
          "tags": [],
          "tagsQuery": "",
          "type": "query",
          "useTags": false
        },
        {
          "allValue": null,
          "current": {
            "text": "All",
            "value": "$__all"
          },
          "datasource": "Prometheus",
          "definition": "label_values(athena_interactions_total, interaction_type)",
          "hide": 0,
          "includeAll": true,
          "label": "Interaction Type",
          "multi": true,
          "name": "interaction_type",
          "options": [],
          "query": "label_values(athena_interactions_total, interaction_type)",
          "refresh": 1,
          "regex": "",
          "skipUrlSync": false,
          "sort": 1,
          "type": "query"
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "builtIn": 1,
          "datasource": "-- Grafana --",
          "enable": true,
          "hide": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "name": "Annotations & Alerts",
          "type": "dashboard"
        }
      ]
    },
    "description": "Comprehensive monitoring dashboard for Sweet Athena AI assistant, including interaction metrics, performance data, user satisfaction, and system health.",
    "gnetId": null,
    "version": 1
  }
}