# AB-MCTS Configuration Example

# Option 1: Use OpenAI
OPENAI_API_KEY=sk-your-openai-key-here

# Option 2: Use Anthropic Claude
ANTHROPIC_API_KEY=sk-ant-your-anthropic-key-here

# Option 3: Use Ollama (local, free)
OLLAMA_URL=http://localhost:11434

# For testing without API keys
USE_MOCK_LLM=true

# AB-MCTS specific settings
AB_MCTS_MAX_ITERATIONS=1000
AB_MCTS_TIME_LIMIT=20000
AB_MCTS_EXPLORATION_CONSTANT=1.414
AB_MCTS_ENABLE_LEARNING=true
AB_MCTS_ENABLE_VISUALIZATION=true