# Agent Coordination Service Configuration

# Server Configuration
RUST_LOG=agent_coordination_service=debug,info
HOST=0.0.0.0
PORT=8080

# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/universal_ai_tools
REDIS_URL=redis://localhost:6379

# Service Configuration
MAX_COLLABORATION_SESSIONS=1000
SESSION_TIMEOUT_SECONDS=3600
AGENT_LOAD_TIMEOUT_SECONDS=30

# External Service URLs
AB_MCTS_SERVICE_URL=http://localhost:8001
PARAMETER_ANALYTICS_SERVICE_URL=http://localhost:8002
ML_INFERENCE_SERVICE_URL=http://localhost:8003

# Performance Settings
WORKER_THREADS=4
MAX_BLOCKING_THREADS=16
ENABLE_METRICS=true

# Security
ENABLE_AUTH=false
JWT_SECRET=your-secret-key-here