# Test Environment Configuration
# Used by test scripts for consistent testing environment

# Server Configuration
PORT=9999
NODE_ENV=testing
BASE_URL=http://localhost:9999

# API Keys (Test values - replace with actual test keys)
DEV_API_KEY=test-api-key-secure-12345
TEST_API_KEY=test-api-key-secure-12345

# Supabase Test Configuration (if needed)
SUPABASE_URL=http://localhost:54321
SUPABASE_ANON_KEY=test-anon-key-for-load-testing-12345678
SUPABASE_SERVICE_KEY=test-service-role-key-for-load-testing-secure-key-123456789

# Database Test Configuration
DATABASE_URL=postgresql://postgres:postgres@localhost:54322/postgres

# Security Configuration
CORS_ORIGINS=http://localhost:5173,http://localhost:3000
JWT_SECRET=test-jwt-secret-key-for-testing-32-chars-minimum-length
ENCRYPTION_KEY=test-encryption-key-for-testing-32-chars-minimum-length

# Redis Configuration
REDIS_URL=redis://localhost:6379

# Rate Limiting
RATE_LIMIT_MAX_REQUESTS=100
RATE_LIMIT_WINDOW_MS=900000

# Performance Testing
PERFORMANCE_MONITORING_ENABLED=true
METRICS_COLLECTION_ENABLED=true

# Test Flags
ENABLE_TEST_ENDPOINTS=true
MOCK_EXTERNAL_SERVICES=true
LOG_LEVEL=debug

# Report Configuration
GENERATE_PERFORMANCE_CHARTS=true
INCLUDE_DETAILED_LOGS=true
EXPORT_TEST_DATA=true