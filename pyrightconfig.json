{
  "include": [
    "src/services/dspy-orchestrator"
  ],
  "exclude": [
    "**/node_modules/**",
    "**/__pycache__/**",
    "**/build/**",
    "**/dist/**",
    "venv/**",
    "**/test_*",
    "**/*_test.py",
    "archive/**",
    "backups/**",
    "ui/**",
    "models/**",
    "coverage/**",
    "datasets/**",
    "temp/**",
    "test-results/**",
    "uploads/**",
    "worktrees/**",
